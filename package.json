      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "foreman": "^3.0.1",
    "electron": "^11.1.0",
    "electron-builder": "^22.9.1"
  }
}
